{
  "name": "DurakElectron",
  "version": "0.0.1",
  "description": "Durak ist ein russisches Kartenspiel welches mit mehreren Personen gespielt wird. Ziel der Applikation ist es, als Unterhaltungsmedium zu fungieren und den Anwender somit zu begeistern. Es soll das Spielen gegen KI gesteuerte Gegner, sowie auch realen menschlichen Gegnern per Internetverbindung erm√∂glichen.",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack Durak.app/Contents/Resources/app Durak.app/Contents/Resources/app.asar",
    "build": "electron-packager ./ Durak --ignore=node_modules/electron-* --ignore=compiled --ignore=.gitignore --all --asar --out=dist --win32metadata.ProductName='Durak' --win32metadata.CompanyName='Nikoai & Andrej' --overwrite"
  },
  "repository": "https://github.com/AndrejSperling/DurakElectron",
  "keywords": [
    "Electron",
    "Durak"
  ],
  "author": "Nikolai Kloss, Andrej Sperling",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/interact.js": "*",
    "@types/jquery": "*",
    "@types/jqueryui": "*",
    "@types/socket.io-client": "^1.4.29",
    "electron-packager": "^8.2.0",
    "electron-prebuilt": "^1.4.5",
    "jquery": "*",
    "jquery-ui": "*",
    "remote": "^0.2.6",
    "socket.io-client": "^1.7.2",
    "typescript": "^2.1.4"
  },
  "devDependencies": {
    "typings": "^2.1.0"
  }
}
